;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

_global_script_classes=[{
"base": "Resource",
"class": &"ResourceOrderRole",
"language": &"GDScript",
"path": "res://Custom Resource/ResourceOrderRole.gd"
}, {
"base": "Resource",
"class": &"ResourcePlayers",
"language": &"GDScript",
"path": "res://Custom Resource/ResourcePlayer.gd"
}, {
"base": "Resource",
"class": &"ResourceRoles",
"language": &"GDScript",
"path": "res://Custom Resource/ResourceRoles.gd"
}]
_global_script_class_icons={
"ResourceOrderRole": "",
"ResourcePlayers": "",
"ResourceRoles": ""
}

[application]

config/name="Werewolf Moderator Tool v4"
run/main_scene="res://Scene/SceneSwitcher.tscn"
config/features=PackedStringArray("4.0")
config/icon="res://icon.png"

[display]

window/size/viewport_width=1080
window/size/viewport_height=1920
window/size/window_width_override=576
window/size/window_height_override=1024
window/handheld/orientation=6
window/stretch/mode="viewport"
window/stretch/aspect="expand"
window/size/width=1080
window/size/height=1920
window/dpi/allow_hidpi.Android=false

[editor_plugins]

enabled=PackedStringArray()

[gui]

common/default_scroll_deadzone=10
fonts/stretch/mode="canvas_items"

[input_devices]

pointing/emulate_touch_from_mouse=true

[physics]

common/enable_pause_aware_picking=true

[rendering]

environment/default_environment="res://Scene/default_env.tres"
